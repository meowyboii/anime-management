<section>
  <h1>Animes</h1>
  <ul>
    <% animes.forEach((anime) => { %>
    <a href="/anime/<%= anime.id %>"
      ><li>
        <strong>Title:</strong> <%= anime.title %><br />
        <strong>Description:</strong> <%= anime.description %><br />
        <strong>Release Date:</strong> <%= anime.release_date %><br />
        <strong>Rating:</strong> <%= anime.rating %><br />
        <strong>Categories:</strong> <%= anime.categories %>
      </li></a
    >
    <hr />
    <% }); %>
  </ul>
</section>
